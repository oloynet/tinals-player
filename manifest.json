{
    "manifest_version": 3,
    "version": "v1.04",
    "version_name": "player v1.04",
    "name": "TINALS 2026",
    "short_name": "TINALS",
    "description": "Festival This Is Not A Love Song BEAU WEEKEND du 5 au 6 juin 2026 à Nîmes. TINALS est un festival de musiques indépendantes dites 'indie' à Paloma.",


    "id": "tinals-player-2026",
    "scope": "/player/",
    "start_url": "/player/",

    "display_override": ["fullscreen", "minimal-ui"],
    "display": "standalone",
    "orientation": "portrait-primary",

    "background_color": "#000000",
    "theme_color": "#2d91d0",

    "default_locale": "fr",
    "author": "Olivier LOYNET - +33.668055252",
    "url": "https://thisisnotalovesong.fr",
    "homepage_url": "https://thisisnotalovesong.fr",
